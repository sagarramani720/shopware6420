<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Start high to low product discount slider cms service -->
        <service id="ICTECHHightoLowDiscountProductSlider\Core\Content\discountProductsSlider\Cms\DiscountProductSliderCmsElementResolver">
            <tag name="shopware.cms.data_resolver"/>
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Shopware\Core\Content\ProductStream\Service\ProductStreamBuilder"/>
            <argument type="service" id="product.repository"/>
        </service>
        <!-- End high to low product discount slider cms service -->
    </services>
</container>
